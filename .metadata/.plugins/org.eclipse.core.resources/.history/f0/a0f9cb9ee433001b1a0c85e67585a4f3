package controller;

import javafx.fxml.FXML;
import javafx.scene.layout.AnchorPane;
import view.Main;

public class HelpController {
	@FXML
	private AnchorPane menuItemHelp;
	
	public void abrirHelp() {
		try {
			Main.changeScene("ajuda");			
		} catch (Exception e){
			e.printStackTrace();
			e.getCause();
		}

}

